# register some connection factories
# connectionfactory.[jndiname] = [ConnectionURL]
<%- if @apply_publisher_specific_configurations -%>
<%- if @apim_traffic_manager['jms_connection_url_1'] -%>
connectionfactory.TopicConnectionFactory = <%= @apim_traffic_manager['jms_connection_url_1'] %>
<%- else -%>
connectionfactory.TopicConnectionFactory = amqp://<%= @apim_traffic_manager['username'] %>:<%= @apim_traffic_manager['password'] %>@clientid/carbon?brokerlist='tcp://<%= @apim_traffic_manager['host'] %>:<%= @apim_traffic_manager['jms_url_port'] %>'
<%- end -%>
<%- end -%>
topic.throttleData = throttleData
connectionfactory.QueueConnectionFactory = amqp://admin:admin@clientID/test?brokerlist='tcp://localhost:5672'

# register some queues in JNDI using the form
# queue.[jndiName] = [physicalName]
#queue.MyQueue = example.MyQueue

# register some topics in JNDI using the form
# topic.[jndiName] = [physicalName]
topic.throttleData = throttleData
